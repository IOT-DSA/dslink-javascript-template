{
  "name": "name-of-template-dslink-javascript",
  "version": "2.0.0",
  "description": "A template to kickstart creating a DSLink using the JavaScript SDK.",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "getDependencies": [
    "node install.js"
  ],
  "configs": {
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "log": {
      "type": "enum",
      "default": "info"
    }
  }
}
